<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="report_library">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2>Report title</h2>
                        <p>This librarys name is
                            <span t-field="o.library_name"/>
                        </p>
                    </div>
                </t>
            </t>
        </t>
    </template>

</odoo>