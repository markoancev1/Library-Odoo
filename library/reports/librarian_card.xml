<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="report_librarian">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2>Report title</h2>
                        <p>This librarians first name is
                            <span t-field="o.librarian_firstname"/>
                        </p>
                        <p>This librarians last name is
                            <span t-field="o.librarian_lastname"/>
                        </p>
                    </div>
                </t>
            </t>
        </t>
    </template>

</odoo>